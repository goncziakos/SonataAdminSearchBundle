{
    "name": "sonata-project/admin-search-bundle",
    "type": "symfony-bundle",
    "description": "Implement Search Engine (ElasticSearch) inside Sonata Admin",
    "keywords": [
        "Admin",
        "search",
        "sonata"
    ],
    "homepage": "https://docs.sonata-project.org/projects/SonataAdminSearchBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "http://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataAdminBundle/contributors"
        }
    ],
    "require": {
        "php": ">=8.1",
        "friendsofsymfony/elastica-bundle": "^6.4",
        "pagerfanta/pagerfanta": "^2.4",
        "ruflin/elastica": "^7.1",
        "sonata-project/admin-bundle": "^4.30.2",
        "symfony/config": "^5.4|^6.4",
        "symfony/dependency-injection": "^5.4|^6.4",
        "symfony/http-kernel": "^5.4|^6.4",
        "symfony/form": "^5.4|^6.4",
        "sonata-project/doctrine-orm-admin-bundle": "^4.9"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^5.2",
        "matthiasnoback/symfony-dependency-injection-test": "^5.1",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.4"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sonata\\AdminSearchBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\AdminSearchBundle\\Tests\\": "tests/"
        }
    },
    "abandoned": true
}
